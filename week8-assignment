// function to reverse a string
function reverseString(oghenetejiri) {
  let reversedWord = "";
  for (let i = oghenetejiri.length - 1; i >= 0; i--) {
    reversedWord += oghenetejiri[i];
  }
  let output = "The reverse of " + oghenetejiri + " is " + reversedWord;
  return output;
}

console.log(reverseString("oghenetejiri"));
